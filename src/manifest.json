{
	"manifest_version": 3,
	"name": "Coub Enhancer",
	"description": "The extension adds additional settings to coub.com",
	"author": "Shikistrafe",
	"version": "0.0.1",
	"action": {
		"default_popup": "/popup/popup.html"
	},
	"content_scripts": [
		{
		"matches": ["https://coub.com/*"],
		"js": ["main.js"]
		}
	],
	"background": {
		"service_worker": "background.js"
	},
	"homepage_url": "https://github.com/Shikistrafe/coub-enhancer",
	"permissions": ["tabs"]
}